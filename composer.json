{
    "name": "huang_chun/transform-api",
    "description": "For APi Transform",
    "type": "library",
    "require": {
        "php": "^8.0",
        "symfony/http-foundation": "^5.0|^6.0",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/pagination": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.3",
        "symfony/var-dumper": "^6.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HuangChun\\TransformApi\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuangChun\\ApiTransform\\TransformServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
